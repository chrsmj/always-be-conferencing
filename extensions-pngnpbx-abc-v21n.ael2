/* The "key-read" macro gets first 16 characters from the key file. */
macro pngnpbx-abc-key-read() {
    Set(LOCAL(fn)="pngnpbx-abc-key.txt");
    Set(LOCAL(fp)=${AST_CONFIG(abc.conf,general,keydir)});
    if( ${LEN(${fp})} == 0 ) {
        Set(fp="/var/lib/asterisk/keys");
    }
    Set(abckey=${FILE(${fp}/${fn},0,16)});
    if( ${LEN(${abckey})} != 16 ) {
        Set(abckey=);
    }
    return;
}

/* The "key-gen" macro generates a new key and saves it in to a file. */
/* It should only be entered infrequently, such as once during initialization,
 * if at all eg. you generate your own keys separately outside of ASTERISK. */
macro pngnpbx-abc-key-gen(fpath_) {
    Set(LOCAL(fn)="pngnpbx-abc-key.txt");
    Set(LOCAL(fp)=${AST_CONFIG(abc.conf,general,keydir)});
    if( ${LEN(${fp})} == 0 ) {
        Set(fp="/var/lib/asterisk/keys");
    }
    local HeFightsHard=;
    local ToDefend=;
    local OurLiberty=;
    local Voluntaryism=;
    local NonAggressionPrinciple=;
    local WiltChamberlainProblem=;
    local BrokenWindowsFallacy=;
    local WhoWillBuildTheRoads=;
    local ConstitutionIsLock=;
    local UsersIsKey=;

    if( ${LEN(${fpath_})} > 0 ) {
        Set(fp=${fpath_});
    }

    Set(ConstitutionIsLock=${fp}/${fn});
    Set(UsersIsKey=${FILE(${ConstitutionIsLock},0,16)});

    // only overwrite existing file if it is too short of a key value
    if( ${LEN(${UsersIsKey})} >= 16 ) {
        Log(WARNING,Detected good existing key for encrypted at rest location data. So not changing the key.);
    } else {
        Log(WARNING,Creating new key for encrypting future at rest location data.);
        Set(HeFights=THANK YOU RAND ${RAND(1,1000000)} PAUL ${RAND(1,1000000)});
        Set(HeFightsHard=${HeFights} FOR DEFENDING THE 6TH AMENDMENT);
        Set(ToDefend=${MD5(${HeFightsHard} a very ${UNIQUEID})});
        Set(OurLiberty=${MD5(${HeFightsHard} in our ${EPOCH})});
        Set(Voluntaryism=${ToDefend}${OurLiberty});
        Set(NonAggressionPrinciple=${LEN(${Voluntaryism})});
        Set(WiltChamberlainProblem=${RAND(1,$[${NonAggressionPrinciple}-17])});
        Set(BrokenWindowsFallacy=${Voluntaryism:${WiltChamberlainProblem}:16});
        Set(WhoWillBuildTheRoads=${STRREPLACE(BrokenWindowsFallacy,=,0)});
        Set(FILE(${ConstitutionIsLock})=${WhoWillBuildTheRoads});
    }

    return;
}

/* The "marco" macro is used to update phone GPS position. */
macro pngnpbx-abc-marco(ext_,plc_,lat_,lng_,elv_,uni_,cpx_,csx_,frx_,clx_,ngx_) {
    local egps=;
    local plaintext=;
    &pngnpbx-abc-key-read();
    if( ${LEN(${abckey})} == 0 ) {
        // Must have a key to encrypt the data with.
        // Key must be 16 characters. Or less.
        // TODO: generate one automatically (hmmmm)
        return;
    }
    if( ${LEN(${ext_})} == 0 ) {
        // Must have an extension number.
        // TODO: lift it from the caller ID number
        return;
    }
    if( ${LEN(${plc_})} == 0 ) {
        Set(plc_=);
        if( ${LEN(${GLOBAL(PNGNPBX_GEO_PLUSCODE)})} > 0 ) {
            Set(plc_=${GLOBAL(PNGNPBX_GEO_PLUSCODE)});
        } else {
            if( ${LEN(${lat_})} == 0 || ${LEN(${lng_})} == 0 ) {
                if( ${LEN(${GLOBAL(PNGNPBX_GEO_LATITUDE)})} > 0 ) {
                    Set(lat_=${GLOBAL(PNGNPBX_GEO_LATITUDE)});
                    Set(lng_=${GLOBAL(PNGNPBX_GEO_LONGITUDE)});
                }
            }
        }
    }
    Set(PUSH(plaintext,|)=plc:${plc_});
    Set(PUSH(plaintext,|)=lat:${lat_});
    Set(PUSH(plaintext,|)=lng:${lng_});
    Set(PUSH(plaintext,|)=elv:${elv_}); // elv + uni combine into NGX
    Set(PUSH(plaintext,|)=uni:${uni_}); // unv + uni combine into NGX
    Set(PUSH(plaintext,|)=cpx:${cpx_});
    Set(PUSH(plaintext,|)=csx:${csx_});
    Set(PUSH(plaintext,|)=frx:${frx_});
    Set(PUSH(plaintext,|)=clx:${clx_});
    Set(PUSH(plaintext,|)=ngx:${ngx_});
    Set(PUSH(plaintext,|)=pep:${RAND()}${RAND()}); // pepper
    Set(egps=${AES_ENCRYPT(${abckey},${plaintext})});
    Set(DB(pngnpbx/abc/egps/${ext_}/crypt)=${egps});
    Set(DB(pngnpbx/abc/egps/${ext_}/touch)=${EPOCH});
    return;
}

/* The "polo" macro is used to retrieve current phone GPS position
 * as it is stored in the internal ASTERISK DB. They must have called
 * out to "marco" sometime before "polo".
 *
 * Parameters:
 * ext_         The extension number to lookup.
 * fulldata_    Set to "full" to return all data.
 *
 * Returns:
 * - values stored in HASH named 'abcd'
 */
macro pngnpbx-abc-polo(ext_,fulldata_) {
    local titles="plc lat lng elv uni cpx csx frx cli ngx";
    local i=;
    local k=;
    local v=;
    local kv=;
    local fn=;
    local fq=;
    local gotnewformat=0;
    Set(HASH(abcd,xyz)=no);
    &pngnpbx-abc-key-read();
    if( ${LEN(${abckey})} == 0 ) {
        // Must have a key to decrypt the data with.
        return;
    }
    if( ${LEN(${ext_})} == 0 ) {
        // Must have an extension number.
        return;
    }
    Set(LOCAL(egps)=${DB(pngnpbx/abc/egps/${ext_}/crypt)});
    if( ${LEN(${egps})} == 0 ) {
        Log(ERROR,Asked to decrypt location but nothing stored!);
    } else if( "x${fulldata_}" != "xfull" ) {
        Set(HASH(abcd,xyz)=yes); // TODO this could be a no
    } else {
        Set(HASH(abcd,xyz)=yes);
        Set(LOCAL(dgps)=${AES_DECRYPT(${abckey},${egps})});
        Set(fq=${FIELDQTY(dgps, )});
        if( ${fq} > 1 ) {
            // k1:v1 k2:v2 ... is ABC version 16 something
            for( i=1; ${i} <= ${fq}; i=${i} + 1 ) {
                Set(kv=${CUT(dgps, ,${i})});
                Set(k=${CUT(kv,:,1)});
                Set(v=${CUT(kv,:,2)});
                Set(fn=${FIELDNUM(titles, ,${k})});
                if( ${fn} > 0 ) {
                    Set(HASH(abcd,${k})=${v});
                    Set(gotnewformat=1);
                }
            }
        } else {
            // k1:v1|k2:v2|... is ABC version 21n
            Set(fq=${FIELDQTY(dgps,|)});
            for( i=1; ${i} <= ${fq}; i=${i} + 1 ) {
                Set(kv=${CUT(dgps,|,${i})});
                Set(k=${CUT(kv,:,1)});
                Set(v=${CUT(kv,:,2)});
                Set(fn=${FIELDNUM(titles, ,${k})});
                if( ${fn} > 0 ) {
                    Set(HASH(abcd,${k})=${v});
                    Set(gotnewformat=2);
                }
            }
        }
        // assume old format if nothing found yet in new formats
        if( ${gotnewformat} == 0 ) {
            // v1 v2 ... was before ABC version 16-ish
            if( ${fq} == 3 ) {
                Set(HASH(abcd,plc)=${CUT(dgps, ,1)});
                Set(HASH(abcd,lat)=); // TODO compute on the fly
                Set(HASH(abcd,lng)=); // TODO compute on the fly
                Set(HASH(abcd,elv)=${CUT(dgps, ,2)});
                Set(HASH(abcd,uni)=${CUT(dgps, ,3)});
            } else {
                Set(HASH(abcd,plc)=); // TODO compute on the fly
                Set(HASH(abcd,lat)=${CUT(dgps, ,1)});
                Set(HASH(abcd,lng)=${CUT(dgps, ,2)});
                Set(HASH(abcd,elv)=${CUT(dgps, ,3)});
                Set(HASH(abcd,uni)=${CUT(dgps, ,4)});
            }
        }
    }
    return;
}

/* The "check-marco" macro looks at last time the extension GPS was updated.
 * If not recently enough, then it re-directs to the IVR. */
macro pngnpbx-abc-check-marco-polo-days(ext_,days_) {
    Set(LOCAL(touched)=${DB(pngnpbx/abc/egps/${ext_}/touch)});
    Set(LOCAL(recently)=${MATH(${days_}*24*60*60,int)});
    if( ${LEN(${touched})} > 0 ) {
        Set(difftouch=${MATH(${EPOCH}-${touched},int)});
        if( ${difftouch} < ${recently} ) {
            return;
        }
    }
    Return(needschange); // sets GOSUB_RETVAL on return
    return;
}

/* The "geohdrs" macro is only utilized in tandem with "gps" macro. */
macro pngnpbx-abc-geohdrs(abcid_) {
    // TODO: check if this is an outgoing PJSIP channel eg. not chan_sip
    // TODO: PIDF-LO support
    Set(LOCAL(plc)=${DB(pngnpbx/abc/cb/${abcid_}/gps/plc)});
    Set(LOCAL(lat)=${DB(pngnpbx/abc/cb/${abcid_}/gps/lat)});
    Set(LOCAL(lng)=${DB(pngnpbx/abc/cb/${abcid_}/gps/lng)});
    Set(LOCAL(elv)=${DB(pngnpbx/abc/cb/${abcid_}/gps/elv)});
    Set(LOCAL(uni)=${DB(pngnpbx/abc/cb/${abcid_}/gps/uni)});
    Set(LOCAL(url)=${DB(pngnpbx/abc/cb/${abcid_}/gps/url)});
    Set(LOCAL(hdr)=${DB(pngnpbx/abc/cb/${abcid_}/gps/hdr)});
    // The Geolocation header is defined in RFC 6442
    if( ${LEN(${url}${lat}${lng})} > 0 ) {
        Set(PJSIP_HEADER(add,Geolocation)=<${url}?lat=${lat}&lng=${lng}>);
        Set(PJSIP_HEADER(add,Geolocation-Routing)=no);
    }
    // Also support non-standard customizable headers
    // TOOD: make these per-trunk specific - not in this macro tho
    for( i=0; ${i} < 10; i=${i} + 1 ) {
        Set(p=${AST_CONFIG(abc.conf,eoc,locationheader,${i})});
        if( ${LEN(${p})} == 0 ) {
            break;
        } else {
            Set(PJSIP_HEADER(add,${p})=${uni} ${elv});
        }
    }
    return;
}

