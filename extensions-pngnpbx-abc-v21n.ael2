exten => 922/123,2,Set(CALLERID(num)=#YOUR-CALLERID-NUM-1##CSX-CODE-FOR-EXTENSION-123#)
exten => 922/456,2,Set(CALLERID(num)=#YOUR-CALLERID-NUM-2##CSX-CODE-FOR-EXTENSION-456#)
exten => 922/789,2,Set(CALLERID(num)=#YOUR-CALLERID-NUM-3##CSX-CODE-FOR-EXTENSION-789#)
exten => 922,2,Set(CALLERID(num)=#YOUR-CALLERID-NUM-0##CSX-CODE-DEFAULT#)
