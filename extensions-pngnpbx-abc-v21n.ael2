/* PUBLIC */
/* The "ivr-diy-gps" context is for users to self update their phone location.
 * Interactive Voice Response - Do It Yourself - Global Positioning System
 * Provides Location Generator (LG) role per RFC 6280 Section 3.2 definition.
 * Accept precision of ~100m down to 1m (3-5 decimal places)
 * TODO: allow copy of co-ords from another user (with or without confirmation)
 * TODO: maybe generate an email whenever a user updates their location.
 * TODO: error checking on all values
 * TODO: lock down who can enter this IVR. */
context pngnpbx-abc-ivr-diy-gps {
    s => {
        &pngnpbx-abc-clu(123*ABC,Welcome to ABC!);
        DumpChan(5);
        Answer();
        WaitExten(1);
        Set(scpx=);
        Set(scsx=);
        Set(sfrx=);
        Set(sclx=);
        Set(scli=);
        Set(sngx=);
        Set(splc=);
        Set(slat=);
        Set(slng=);
        Set(suni=);
        Set(unitypeid=);
        Set(selv=);
        Set(TIMEOUT(digit)=2);
    getinfo:
        &pngnpbx-abc-clu(123*ABC,Main Menu);
        Background(hello&get_information);
        // several options...
        // These two are mutually exclusive:
        // 1. Lat/Lng (GPS - default)
        // 2. PLC Plus Codes (like GPS but better)
        // These five are optional and can be combined:
        // 1. CPX code (prefix the caller ID number)
        // 2. CSX code (suffix the caller ID number)
        // 3. FRX code (TBD)
        // 4. NGX codes (units/elevation/etc. eg. Room 1234)
        // 5. CLIPS codes (TBD)
    gps:
        &pngnpbx-abc-clu(Press-Star-0,Use GPS ?);
        Background(for);
        Background(letters/g);
        Background(letters/p);
        Background(letters/s);
        Background(vm-press&digits/star&digits/0);
        WaitExten(1);
    cpx:
        &pngnpbx-abc-clu(Press-Star-1,Use CPX Code ?);
        Background(for);
        Background(letters/c);
        Background(letters/p);
        Background(letters/x);
        Background(vm-press&digits/star&digits/1);
        WaitExten(1);
    csx:
        &pngnpbx-abc-clu(Press-Star-2,Use CSX Code ?);
        Background(for);
        Background(letters/c);
        Background(letters/s);
        Background(letters/x);
        Background(vm-press&digits/star&digits/2);
        WaitExten(1);
    frx:
        &pngnpbx-abc-clu(Press-Star-3,Use FRX Code ?);
        Background(for);
        Background(letters/f);
        Background(letters/r);
        Background(letters/x);
        Background(vm-press&digits/star&digits/3);
        WaitExten(1);
    plc:
        &pngnpbx-abc-clu(Press-Star-4,Use PLUS Code ?);
        Background(for);
        Background(letters/plus);
        Background(encode);
        Background(vm-press&digits/star&digits/4);
        WaitExten(1);
    cli:
        &pngnpbx-abc-clu(Press-Star-5,Use CLIPS Code ?);
        Background(for);
        Background(letters/c);
        Background(letters/l);
        Background(letters/i);
        Background(letters/p);
        Background(letters/s);
        Background(encode);
        Background(vm-press&digits/star&digits/5);
        WaitExten(1);
    ngx:
        &pngnpbx-abc-clu(Press-Star-6,Use NGX ?);
        Background(for);
        Background(letters/n);
        Background(letters/g);
        Background(letters/x);
        Background(units&ha/room&or&ha/floor&number);
        Background(vm-press&digits/star&digits/6);
        WaitExten(3);
        Background(moving&on);
    lat:
        WaitExten(1);
        Background(letters/g&letters/p&letters/s);
        WaitExten(1);
        &pngnpbx-abc-clu(39*7394,Latitude ?);
        Background(please-enter-the);
        Background(letters/l&letters/a&letters/t);
        Background(letters/i&letters/t&letters/u&letters/d&letters/e);
        Background(then-press-pound);
        Background(for&letters/dot&vm-press&digits/star);
        WaitExten(1);
        Background(with&negative&number);
        Background(please-try&digits/0);
        Background(before-the-number); // so 075.757 equals -75.757
        WaitExten(2);
        Background(letters/l&letters/a&letters/t);
        Background(letters/i&letters/t&letters/u&letters/d&letters/e);
        WaitExten(10);
        Background(goodbye);
        Hangup();
    lng:
        &pngnpbx-abc-clu(0104*990,Longitude ?);
        Background(now);
        Background(letters/l&letters/o&letters/n&letters/g);
        Background(letters/i&letters/t&letters/u&letters/d&letters/e);
        WaitExten(2);
        Background(letters/l&letters/o&letters/n&letters/g);
        Background(letters/i&letters/t&letters/u&letters/d&letters/e);
        WaitExten(10);
        Background(goodbye);
        Hangup();
    address_type:
        NoOp(uni compatibility line);
    uni:
        // uni <-> address_type TODO Translations:
        // Feet 123 <-> Key - Feet 123
        // Meters 456 <-> Key - Meters 456
        // Plus Code 85FQP2Q6+H7 <-> Key - https://plus.codes/85FQP2Q6+H7
        // GPS 39.588x-105.644 <-> Key - Latitude 39.588 Longitude -105.644
        Set(unitypes=);
        Set(PUSH(unitypes,|)=Feet-/feet);
        Set(PUSH(unitypes,|)=Meters-/meters);
        Set(PUSH(unitypes,|)=Apartment-a.p.a.r.t.m.e.n.t.);
        Set(PUSH(unitypes,|)=Basement-/ha/basement);
        Set(PUSH(unitypes,|)=Building-b.u.i.l.d.i.n.g.);
        Set(PUSH(unitypes,|)=Department-d.e.p.a.r.t.m.e.n.t.);
        Set(PUSH(unitypes,|)=Floor-/ha/floor);
        Set(PUSH(unitypes,|)=Front-/ha/front);
        Set(PUSH(unitypes,|)=Key-k.e.y.);
        Set(PUSH(unitypes,|)=Lobby-l.o.b.b.y.);
        Set(PUSH(unitypes,|)=Lot-l.o.t.);
        Set(PUSH(unitypes,|)=Lower-l.o.w.e.r.);
        Set(PUSH(unitypes,|)=Office-/office);
        Set(PUSH(unitypes,|)=Penthouse-/presidents-office); // could be better
        Set(PUSH(unitypes,|)=Pier-p.i.e.r.);
        Set(PUSH(unitypes,|)=Rear-r.e.a.r.);
        Set(PUSH(unitypes,|)=Room-/ha/room);
        Set(PUSH(unitypes,|)=Side-/side);
        Set(PUSH(unitypes,|)=Slip-s.l.i.p.);
        Set(PUSH(unitypes,|)=Space-/space);
        Set(PUSH(unitypes,|)=Stop-s.t.o.p.);
        Set(PUSH(unitypes,|)=Suite-s.u.i.t.e.);
        Set(PUSH(unitypes,|)=Trailer-t.r.a.i.l.e.r.);
        Set(PUSH(unitypes,|)=Unit-/units);
        Set(PUSH(unitypes,|)=Upper-u.p.p.e.r.);
        Set(fqunitypes=${FIELDQTY(unitypes,|)});
        for( i=1; ${i} <= ${fqunitypes}; i=${i} + 1 ) {
            Set(lblsnd=${CUT(unitypes,|,${i})});
            Set(lbl=${CUT(lblsnd,-,1)});
            Set(snd=${CUT(lblsnd,-,2)});
            Set(HASH(abcuniopts,${i})=${lbl});
            Set(HASH(abcunioptsnds,${i})=${snd});
        }
        Set(unitypeid=0);
        &pngnpbx-abc-clu(Options-1-25,Units ?);
        Background(astcc-please-enter-your);
        Background(location&position&units);
        Background(now);
        for( i=1; ${i} <= ${fqunitypes}; i=${i} + 1 ) {
            Set(lblsnd=${CUT(unitypes,|,${i})});
            Set(lbl=${CUT(lblsnd,-,1)});
            Set(snd=${CUT(lblsnd,-,2)});
            &pngnpbx-abc-clu(Press-${i},${lbl});
            if( ${FIELDQTY(snd,/)} > 1 ) {
                Background(${snd:1});
            } else {
                Set(lsnd=${FIELDQTY(snd,.)});
                for( j=1; ${j} < ${lsnd}; j=${j} + 1) {
                    Set(l=${CUT(snd,.,${j})});
                    Background(letters/${l});
                } 
            }
            NoOp(i is now ${i});
            if( ${i} > 25 ) {
                Log(ERROR,Option out of range.);
            } else if( ${i} >= 20 ) {
                Background(press-2&digits/${i:1});
            } else if( ${i} >= 10 ) {
                Background(press-1&digits/${i:1});
            } else {
                Background(press-${i});
            }
        }
        WaitExten(2);
        &pngnpbx-abc-clu(Options-1-25,Units ?);
        Background(units&units); // not wearing pants
        WaitExten(1);
        &pngnpbx-abc-clu(Repeat-Press-Star,Units ?);
        Background(to-hear-menu-again&press-star);
        WaitExten(10);
        Background(goodbye);
        Hangup();
    address_type_number:
        NoOp(elv compatibility line);
    elv:
        // TODO: cardinal directions N NE E SE S SW W NW
        // (only numbers are currently supported)
        &pngnpbx-abc-clu(1234,${suni} ?);
        Background(now);
        Background(please-enter-your);
        Set(snd=${sunisnd});
        if( ${FIELDQTY(snd,/)} > 1 ) {
            Background(${snd:1});
        } else {
            Set(lsnd=${FIELDQTY(snd,.)});
            for( j=1; ${j} < ${lsnd}; j=${j} + 1) {
                Set(l=${CUT(snd,.,${j})});
                Background(letters/${l});
            } 
        }
        Background(number);
        WaitExten(2);
        &pngnpbx-abc-clu(0123,Negative Number ?);
        Background(negative&number);
        Background(please-try&digits/0);
        Background(before-the-number); // so 075.757 equals -75.757
        WaitExten(10);
        Background(goodbye);
        Hangup();
    label:
        // TODO
        Hangup();
    first_name:
        // TODO
        Hangup();
    last_name:
        // TODO
        Hangup();
    street_number:
        // TODO
        Hangup();
    street_name:
        // TODO
        Hangup();
    city:
        // TODO
        Hangup();
    state:
        // TODO
        Hangup();
    country:
        // TODO
        Hangup();
    zip:
        // TODO
        Hangup();
    }

    // positives
    _X*XXX#! => jump ${EXTEN},gooduser;
    _XX*XXX#! => jump ${EXTEN},gooduser;
    _XXX*XXX#! => jump ${EXTEN},gooduser;
    _X*XXXX#! => jump ${EXTEN},gooduser;
    _XX*XXXX#! => jump ${EXTEN},gooduser;
    _XXX*XXXX#! => jump ${EXTEN},gooduser;
    _X*XXXXX#! => jump ${EXTEN},gooduser;
    _XX*XXXXX#! => jump ${EXTEN},gooduser;
    _XXX*XXXXX#! => jump ${EXTEN},gooduser;
    _X*XXX => jump ${EXTEN}#;
    _XX*XXX => jump ${EXTEN}#;
    _XXX*XXX => jump ${EXTEN}#;
    _X*XXXX => jump ${EXTEN}#;
    _XX*XXXX => jump ${EXTEN}#;
    _XXX*XXXX => jump ${EXTEN}#;
    _X*XXXXX => jump ${EXTEN}#;
    _XX*XXXXX => jump ${EXTEN}#;
    _XXX*XXXXX => jump ${EXTEN}#;
    // negatives (leading zero means minus)
    _0X*XXX#! => jump ${EXTEN},gooduser;
    _0XX*XXX#! => jump ${EXTEN},gooduser;
    _0XXX*XXX#! => jump ${EXTEN},gooduser;
    _0X*XXXX#! => jump ${EXTEN},gooduser;
    _0XX*XXXX#! => jump ${EXTEN},gooduser;
    _0XXX*XXXX#! => jump ${EXTEN},gooduser;
    _0X*XXXXX#! => jump ${EXTEN},gooduser;
    _0XX*XXXXX#! => jump ${EXTEN},gooduser;
    _0XXX*XXXXX#! => jump ${EXTEN},gooduser;
    _0X*XXX => jump ${EXTEN}#;
    _0XX*XXX => jump ${EXTEN}#;
    _0XXX*XXX => jump ${EXTEN}#;
    _0X*XXXX => jump ${EXTEN}#;
    _0XX*XXXX => jump ${EXTEN}#;
    _0XXX*XXXX => jump ${EXTEN}#;
    _0X*XXXXX => jump ${EXTEN}#;
    _0XX*XXXXX => jump ${EXTEN}#;
    _0XXX*XXXXX => jump ${EXTEN}#;
    // above ground
    // and other positive numbers
    _X#! => jump ${EXTEN},gooduser;
    _XX#! => jump ${EXTEN},gooduser;
    _XXX#! => jump ${EXTEN},gooduser;
    _XXXX#! => jump ${EXTEN},gooduser;
    _XXXXX#! => jump ${EXTEN},gooduser;
    _XXXXXX#! => jump ${EXTEN},gooduser;
    _X => jump ${EXTEN}#;
    _XX => jump ${EXTEN}#;
    _XXX => jump ${EXTEN}#;
    _XXXX => jump ${EXTEN}#;
    _XXXXX => jump ${EXTEN}#;
    _XXXXXX => jump ${EXTEN}#;
    // under ground
    // and other negative numbers
    _0X#! => jump ${EXTEN},gooduser;
    _0XX#! => jump ${EXTEN},gooduser;
    _0XXX#! => jump ${EXTEN},gooduser;
    _0XXXX#! => jump ${EXTEN},gooduser;
    _0XXXXX#! => jump ${EXTEN},gooduser;
    _0XXXXXX#! => jump ${EXTEN},gooduser;
    _0X => jump ${EXTEN}#;
    _0XX => jump ${EXTEN}#;
    _0XXX => jump ${EXTEN}#;
    _0XXXX => jump ${EXTEN}#;
    _0XXXXX => jump ${EXTEN}#;
    _0XXXXXX => jump ${EXTEN}#;
    // set to 0 if just hash - fast skip method
    _#! => jump 0#,submitgps;
    // short cut to repeat at start of mini ivr
    _**! => jump s,getinfo;
    // repeats just the unit names and numbers 1-25
    _* => jump s,uni;
    // GPS default - starts with latitude
    _*0! => jump s,lat;
    // CPX CODES - 10 digit PBX Caller ID Prefix (eg. Emergency Call-Back Profile)
    // if defined by user, and trunk uses it per abc.conf,
    // the CPX code will overwrite the Caller ID on this call
    _*1! => {
        ucsx0=;
        ucsx=;
        &pngnpbx-abc-clu(123456,CPX Code ? eg.);
        Read(ucpx0,please-enter-the&letters/c&letters/p&letters/x&encode,10,,3,3);
        Set(ucpx=${FILTER(0-9,${ucpx0})});
        // TODO: check if in allowed list of CPX CODES - with CURL ?
        //&pngnpbx-abc-check-cpx();
        Set(scpx=${ucpx});
        &pngnpbx-abc-clu(${scpx},CPX Code);
        Background(auth-thankyou);
        WaitExten(2);
        goto s,csx;
    }
    // CSX CODES - 6 digit PBX Caller ID Suffix (eg. Dispatchable Locations)
    _*2! => {
        ucsx0=;
        ucsx=;
        &pngnpbx-abc-clu(123456,CSX Code ? eg.);
        Read(ucsx0,please-enter-the&letters/c&letters/s&letters/x&encode,6,,3,3);
        Set(ucsx=${FILTER(0-9,${ucsx0})});
        // TODO: check if in allowed list of CSX CODES - with CURL ?
        //&pngnpbx-abc-check-csx();
        Set(scsx=${ucsx});
        &pngnpbx-abc-clu(${scsx},CSX Code);
        Background(auth-thankyou);
        WaitExten(2);
        goto s,frx;
    }
    // FRX CODES - many digits
    _*3! => {
        ufrx0=;
        ufrx=;
        &pngnpbx-abc-clu(1234567890,FRX Code ? eg.);
        Read(ufrx0,please-enter-the&letters/f&letters/r&letters/x&encode,6,,3,3);
        Set(ufrx=${FILTER(0-9,${ufrx0})});
        //&pngnpbx-abc-check-frx(); // TODO
        Set(sfrx=${ufrx});
        &pngnpbx-abc-clu(${sfrx},FRX Code);
        Background(auth-thankyou);
        WaitExten(2);
        goto s,plc;
    }
    // the PLUS CODES entry section
    _*4! => {
        uplc=;
        j=;
        k=;
        y=;
        z=;
        &pngnpbx-abc-clu(85FQP2Q6*7F,Plus Code ? eg.);
        // assumes 11 digit global code
        // available characters 23456789CFGHJMPQRVWX
        Read(j,please-enter-the&letters/plus&encode,1,,,1);
        // TODO: while loops may be buggy in AEL...
        // maybe make this into a HASH...
        // or replace with Signal()/WaitForSignal()
        while( ${LEN(${uplc})} < 11 ) {
            if( ${LEN(${h})} == 0 ) {
                Read(k,silence/1,1,,,1);
            } else {
                k=${j};
                j=;
            }
            if( ${LEN(${k})} > 0 ) {
                z=${k};
                if( ${k} == 0 ) {        // 0
                    NoOp();
                } else if( ${k} == 1 ) { // 1
                    NoOp();
                } else if( ${k} == 2 ) { // 2-C
                    if( "y${y}" == "y2" ) {
                        z=C;
                    }
                } else if( ${k} == 3 ) { // 3-F
                    if( "y${y}" == "y3" ) {
                        z=F;
                    }
                } else if( ${k} == 4 ) { // 4-G-H
                    if( "y${y}" == "y4" ) {
                        z=G;
                    } else if( "y${y}" == "yG" ) {
                        z=H;
                    }
                } else if( ${k} == 5 ) { // 5-J
                    if( "y${y}" == "y5" ) {
                        z=J;
                    }
                } else if( ${k} == 6 ) { // 6-M
                    if( "y${y}" == "y6" ) {
                        z=M;
                    }
                } else if( ${k} == 7 ) { // 7-P-Q-R
                    if( "y${y}" == "y7" ) {
                        z=P;
                    } else if( "y${y}" == "yP" ) {
                        z=Q;
                    } else if( "y${y}" == "yQ" ) {
                        z=R;
                    }
                } else if( ${k} == 8 ) { // 8-V
                    if( "y${y}" == "y8" ) {
                        z=V;
                    }
                } else if( ${k} == 9 ) { // 9-W-X
                    if( "y${y}" == "y9" ) {
                        z=W;
                    } else if( "y${y}" == "yW" ) {
                        z=X;
                    }
                } else {
                    // ignored key
                    z=;
                }
                if( ${LEN(${z})} > 0 ) {
                    Set(y=${z});
                    &pngnpbx-abc-clu(${z},Code ${uplc});
                }
            } else {
                if( ${LEN(${y})} > 0 ) {
                    Set(uplc=${uplc}${y});
                    if( ${LEN(${uplc})} == 8 ) {
                        Set(uplc=${uplc}+);
                    }
                    &pngnpbx-abc-clu(${y},Code ${uplc});
                    Set(y=);
                }
            }
        }
        Set(splc=${uplc});
        &pngnpbx-abc-clu(${splc},Code ${splc});
        Background(auth-thankyou);
        WaitExten(2);
        goto s,cli;
    }
    // use CLIPS codes 
    _*5! => {
        ucli0=;
        ucli=;
        &pngnpbx-abc-clu(1234567890,CLIPS Code ? eg.);
        Read(ucli0,please-enter-the&letters/c&letters/l&letters/i&letters/p&letters/s&encode,6,,3,3);
        Set(ucli=${FILTER(0-9,${ucli0})});
        //&pngnpbx-abc-check-clips(); // TODO
        Set(scli=${ucli});
        &pngnpbx-abc-clu(${scli},CLIPS Code);
        Background(auth-thankyou);
        WaitExten(2);
        goto s,ngx;
    }
    // use NGX codes
    _*6! => {
        ungx0=;
        ungx=;
        &pngnpbx-abc-clu(1234567890,NGX Code ? eg.);
        Read(ungx0,please-enter-the&letters/n&letters/g&letters/x&encode,6,,3,3);
        Set(ungx=${FILTER(0-9,${ungx0})});
        //&pngnpbx-abc-check-ngx(); // TODO
        Set(sngx=${ungx});
        &pngnpbx-abc-clu(${sngx},NGX Code);
        Background(auth-thankyou);
        WaitExten(2);
        goto s,uni;
    }

    // any hash terminated string
    _X.#! => goto ${EXTEN:0:-1},1;
    _X. => {
        jump i; // disallow . wildcard - it is too vague
        Congestion(10); // srsly no funny business users
        HANGUP(); // SRSLY
    gooduser:
        if( ${LEN(${scpx}${scsx}${sfrx}${splc}${slat}${unitypeid})} == 0 ) {
            // no other location info - default to check latitude
            Set(ulat=${EXTEN:0:-1});
            Set(ulats=${FILTER(*0-9,${ulat})});
            if( "x${ulats:0:1}" == "x0" ) {
                Set(ulats=-${ulats:1});
            }
            Set(ulatd=${STRREPLACE(ulats,*,.)});
            if( ${LEN(${CUT(ulatd,.,2)})} < 3 ) {
                &pngnpbx-abc-clu(${ulats},BAD Latitude ${ulatd});
                Background(confbridge-invalid);
                &pngnpbx-abc-clu(39*555,Use 3-5 digit mantissa);
                Background(minimum&digits);
                SayDigits(3);
                Background(maximum&digits);
                &pngnpbx-abc-clu(39*55555,Use 3-5 digit mantissa);
                SayDigits(5);
                WaitExten(4);
                goto s,lat;
            } else if( ${ulatd} > 90 || ${ulatd} < -90 ) {
                &pngnpbx-abc-clu(${ulats},BAD Latitude ${ulatd});
                Background(confbridge-invalid);
                goto s,lat;
            } else {
                Set(slat=${ulatd});
                &pngnpbx-abc-clu(${ulats},GOOD Latitude ${slat});
                Background(auth-thankyou);
                WaitExten(2);
                goto s,lng;
            }
        }
        if( ${LEN(${scpx}${scsx}${sfrx}${splc}${slng}${unitypeid})} == 0 ) {
            // no other location info - default to check longitude
            Set(ulng=${EXTEN:0:-1});
            Set(ulngs=${FILTER(*0-9,${ulng})});
            if( "x${ulngs:0:1}" == "x0" ) {
                Set(ulngs=-${ulngs:1});
            }
            Set(ulngd=${STRREPLACE(ulngs,*,.)});
            if( ${LEN(${CUT(ulngd,.,2)})} < 3 ) {
                &pngnpbx-abc-clu(${ulngs},BAD Longitude ${ulngd});
                Background(confbridge-invalid);
                Background(minimum&digits);
                &pngnpbx-abc-clu(0104*555,Use 3-5 digit mantissa);
                SayDigits(3);
                Background(maximum&digits);
                SayDigits(5);
                &pngnpbx-abc-clu(0104*55555,Use 3-5 digit mantissa);
                WaitExten(4);
                goto s,lng;
            } else if( ${ulngd} > 180 || ${ulngd} < -180 ) {
                &pngnpbx-abc-clu(${ulngs},BAD Longitude ${ulngd});
                Background(confbridge-invalid);
                goto s,lng;
            } else {
                Set(slng=${ulngd});
                &pngnpbx-abc-clu(${ulngs},GOOD Longitude ${slng});
                Background(auth-thankyou);
                WaitExten(2);
                goto s,uni;
            }
        }
        if( ${LEN(${suni})} == 0 ) {
            Set(uuni=${FILTER(0-9,${EXTEN:0:2})});
            Set(uunix=${HASH(abcuniopts,${uuni})});
            Set(sunisnd=${HASH(abcunioptsnds,${uuni})});
            if( ${LEN(${uunix})} > 0 ) {
                Set(suni=${uunix});
            } else {
                Set(suni=Feet); // default
            }
            &pngnpbx-abc-clu(${suni},Your Chosen Units);
            Background(auth-thankyou);
            WaitExten(1);
            goto s,elv;
        }
        if( ${LEN(${selv})} == 0 ) {
            Set(uelv=${EXTEN:0:-1});
            Set(uelvx=${FILTER(0-9,${uelv})});
            if( "x${uelvx:0:1}" == "x0" ) {
                Set(uelvx=-${uelvx:1});
            }
            Set(selv=${STRREPLACE(uelvx,*,.)});
            &pngnpbx-abc-clu(${selv}${suni},Elev/Dpth/Flr/Rm/Bldg/etc.);
        }
    submitgps:
        &pngnpbx-abc-clu(Press-1-Confirm,Ready to Submit Location Updates ?);
        Set(snd=${sunisnd});
        if( ${FIELDQTY(snd,/)} > 1 ) {
            Playback(${snd:1});
        } else {
            Set(lsnd=${FIELDQTY(snd,.)});
            for( j=1; ${j} < ${lsnd}; j=${j} + 1) {
                Set(l=${CUT(snd,.,${j})});
                Playback(letters/${l});
            } 
        }
        SayDigits(${selv});
        Read(uearlyconf,if-this-is-correct&press-1,1,,2,2);
        if( "x${uearlyconf}" != "x1" ) {
            goto s,getinfo;
        } else {
            &pngnpbx-abc-marco(${abcid},${splc},${slat},${slng},${selv},${suni},${scpx},${scsx},${sfrx},${scli},${sngx});
            &pngnpbx-abc-clu(Hooray,Submitted);
            Background(auth-thankyou);
            goto confirmation,1;
        }
    }

    confirmation => {
        Set(SAY_DTMF_INTERRUPT=false);
        &pngnpbx-abc-clu(123*ABC,Location Confirmation);
        &pngnpbx-abc-polo(${abcid},full);
        &pngnpbx-abc-tones();
        Playback(confirm-number-is&now);
        // TODO maybe merge with other tts
        if( ${LEN(${HASH(abcd,cpx)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,cpx)},CPX CODE);
            SayAlpha(cpx);
            SayDigits(${HASH(abcd,cpx)});
        }
        if( ${LEN(${HASH(abcd,csx)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,csx)},CSX CODE);
            SayAlpha(csx);
            SayDigits(${HASH(abcd,csx)});
        }
        if( ${LEN(${HASH(abcd,frx)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,frx)},FRX CODE);
            SayAlpha(frx);
            SayDigits(${HASH(abcd,frx)});
        }
        if( ${LEN(${HASH(abcd,ngx)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,ngx)},NGX CODE);
            SayAlpha(ngx);
            SayDigits(${HASH(abcd,ngx)});
        }
        if( ${LEN(${HASH(abcd,cli)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,cli)},CLIPS CODE);
            SayAlpha(cli);
            SayDigits(${HASH(abcd,cli)});
        }
        if( ${LEN(${HASH(abcd,plc)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,uni)}${HASH(abcd,elv)},${HASH(abcd,plc)});
            Playback(letters/plus&encode);
            SayAlpha(${HASH(abcd,plc)});
        }
        if( ${LEN(${HASH(abcd,lat)})} > 0 && ${LEN(${HASH(abcd,lng)})} > 0 ) {
            &pngnpbx-abc-clu(${HASH(abcd,uni)}${HASH(abcd,elv)},${HASH(abcd,lat)} ${HASH(abcd,lng)});
            Set(t=${HASH(abcd,lat)});
            Set(g=${HASH(abcd,lng)});
            SayNumber(${CUT(t,.,1)});
            Playback(letters/dot);
            SayDigits(${CUT(t,.,2)});
            Playback(by);
            SayNumber(${CUT(g,.,1)});
            Playback(letters/dot);
            SayDigits(${CUT(g,.,2)});
        }

        if( ${LEN(${HASH(abcd,uni)})} > 0 ) {
            Wait(1);
            Playback(units);
            SayAlpha(${HASH(abcd,uni)});
        }
        if( ${LEN(${HASH(abcd,elv)})} > 0 ) {
            Playback(position);
            SayAlpha(${HASH(abcd,elv)});
            Wait(1);
        }

        &pngnpbx-abc-tones();
        &pngnpbx-abc-clu(Press-*,If NOT correct);
        Read(really,if-this-is-not-correct&press-star&now,1,,,3);
        if( "x${really}" == "x*" ) {
            Playback(no_worries_try_again);
            goto s,1;
        }

        &pngnpbx-abc-clu(1,Thanks);
        &pngnpbx-abc-tones();
        Wait(.1);
        &pngnpbx-abc-clu(12,Thanks for);
        &pngnpbx-abc-tones();
        Wait(.1);
        &pngnpbx-abc-clu(123,Thanks for using);
        &pngnpbx-abc-tones();
        Wait(.1);
        &pngnpbx-abc-clu(123*222,Thanks for using ABC!);
        &pngnpbx-abc-tones();
        Wait(1);
        &pngnpbx-abc-clu(123*ABC,As easy as 123*ABC);
        Playback(silence/1);
        if( ${LEN(${HASH(abcd,plc)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,uni)}${HASH(abcd,elv)},${HASH(abcd,plc)});
        }
        if( ${LEN(${HASH(abcd,lat)})} > 0 && ${LEN(${HASH(abcd,lng)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,uni)}${HASH(abcd,elv)},${HASH(abcd,lat)} ${HASH(abcd,lng)});
        }
        if( ${LEN(${HASH(abcd,cpx)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,cpx)},CPX CODE);
        }
        if( ${LEN(${HASH(abcd,csx)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,csx)},CSX CODE);
        }
        if( ${LEN(${HASH(abcd,frx)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,frx)},FRX CODE);
        }
        if( ${LEN(${HASH(abcd,ngx)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,ngx)},NGX CODE);
        }
        if( ${LEN(${HASH(abcd,cli)})} > 0 ) {
            Wait(2);
            &pngnpbx-abc-clu(${HASH(abcd,cli)},CLIPS CODE);
        }
        Wait(2);
        &pngnpbx-abc-song();
        Playback(silence/1);
        Wait(10);
        Playback(goodbye);
        Hangup();
    }

    t => jump i;
    i => {
        &pngnpbx-abc-clu(,What ?);
        Playback(sorry-youre-having-problems);
        if( ${LEN(${splc})} == 0 ) {
            if( ${LEN(${slat})} == 0 ) {
                Playback(pls-try-again);
                jump s,plc;
            }
            if( ${LEN(${slng})} == 0 ) {
                Playback(pls-try-again);
                jump s,lng;
            }
        }
        if( ${LEN(${suni})} == 0 ) {
            Playback(pls-try-again);
            jump s,uni;
        }
        if( ${LEN(${selv})} == 0 ) {
            Playback(pls-try-again);
            jump s,elv;
        }
        Playback(please-hang-up-and-try-again);
        Playback(goodbye);
        Hangup();
    }

} // end context pngnpbx-abc-ivr-diy-gps

