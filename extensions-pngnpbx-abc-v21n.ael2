/* PUBLIC */
/* EASY INTERFACE */
context pngnpbx-abc-easy-include-gps {
    _123[*#]222 => goto 123*222*1,1; // for end users it is as easy as 123*ABC!
    _123[*#]222[*#]0 => {
        Log(VERBOSE,Always Be Conferencing - PenguinPBX.com - Interface - GPS - 0 - IT Init Keys);
        // TODO: limit to certain extensions
        Answer();
        Set(cidnum=${FILTER(0-9a-zA-Z,${CALLERID(num)})});
        &pngnpbx-abc-account(${cidnum});
        &pngnpbx-abc-tones();
        // must gen key before dynamic GPS will work
        &pngnpbx-abc-key-gen("");
        //Playback(auth-thankyou);
        //Playback(telephone-in-your-pocket); // not wearing pants
        Playback(dial-here-often); // not wearing pants
    }
    _123[*#]222[*#]1 => {
        Log(VERBOSE,Always Be Conferencing - PenguinPBX.com - Interface - GPS - 1 - Self-Serve);
        Answer();
        Set(cidnum=${FILTER(0-9a-zA-Z,${CALLERID(num)})});
        &pngnpbx-abc-account(${cidnum});
        &pngnpbx-abc-tones();
        // has ABC been set up yet by IT dialing 123*222*0 ?
        &pngnpbx-abc-key-read();
        if( ${LEN(${abckey})} == 0 ) {
            Playback(oops1);
            Playback(please-contact-tech-supt);
            Wait(1);
            Playback(vm-goodbye);
            Hangup();
        }
        // did they update GPS in the past year ?
        &pngnpbx-abc-check-marco-polo-days(${cidnum},365);
        if( "x${GOSUB_RETVAL}" == "xneedschange" ) {
            goto pngnpbx-abc-ivr-diy-gps,s,1;
        } else {
            goto pngnpbx-abc-ivr-diy-gps,confirmation,1;
        }
    }
    _123[*#]222[*#]2[*#].=> {
        Log(VERBOSE,Always Be Conferencing - PenguinPBX.com - Interface - GPS - 2 - IT Check User);
        // TODO: limit to certain extensions
        Answer();
        Set(tech=${CHANNEL(channeltype)});
        Set(ext=${FILTER(0-9a-zA-Z,${CUT(EXTEN,*,4)})});
        // did they update GPS in the past year ?
        &pngnpbx-abc-check-marco-polo-days(${ext},365);
        Set(needschange=${GOSUB_RETVAL});
        &pngnpbx-abc-tones();
        if( "x${needschange}" == "xneedschange" ) {
            // Calls the user to tell them to update.
            &pngnpbx-abc-clu(${ext},User);
            SayAlpha(gps);
            &pngnpbx-abc-clu(${ext},User needs);
            Playback(is-not-set);
            &pngnpbx-abc-clu(${ext},User needs update!);
            SayAlpha(ok);
            Wait(1);
            Playback(please-hold-minion-connect);
            &pngnpbx-abc-clu(${ext},Tell them 123*ABC);
            Wait(1);
            // IT can hang up, and the callee will be redirected to the IVR
            Dial(${tech}/${ext},60,iIF(pngnpbx-abc-user-training-mode^${ext}^1));
        } else {
            &pngnpbx-abc-clu(${ext},User Location is OKAY!);
            Playback(position&is);
            SayAlpha(ok);
        }
    }
}

/* PUBLIC */
/* EASY INTERFACE */
context pngnpbx-abc-easy-goto-eoc {
    _[0-9A-Za-z]. => {

    simple:
        Set(abcmode=simple);
        goto common;
    tincan:
        Set(abcmode=tincan);
        goto common;
    parallel:
        Set(abcmode=parallel);
        goto common;
    common:
        Set(cleanext=${FILTER(0-9a-zA-Z,${EXTEN})});
        Set(cleancid=${FILTER(0-9a-zA-Z,${CALLERID(num)})});
        Set(fpbxecid=${DB(DEVICE/${cleancid}/emergency_cid)});
        Set(cpxcode=${DB(DEVICE/${cleancid}/emergency_cpx)});
        Set(csxcode=${DB(DEVICE/${cleancid}/emergency_csx)});
        Set(frxcode=${DB(DEVICE/${cleancid}/emergency_frx)});
        Set(gpsurl=https://plus.codes/);
        Set(dbcb=pngnpbx/abc/cb);
        Set(CALLERID(num)=${cleancid}); // or fpbxcid hmmm
        Set(muhnewtechs=PJSIP SIP);
        Set(muholdtechs=DAHDI IAX);

        if( "x${abcmode}" == "xparallel" ) {
            &pngnpbx-abc-parallel(ABC-EASY-PARA,${cleanext});
            &pngnpbx-abc-gps-get(); // gets GPS from AstDB, if any
            Set(abcdialstr1=Local/${cleanext}@pngnpbx-abc-parallel-vm-notify);
            for( i=0; ${i} < 10; i=${i} + 1 ) {
                Set(p=${AST_CONFIG(abc.conf,eoc,route,${i})});
                if( ${LEN(${p})} == 0 ) {
                    break;
                }
                Set(tech=${CUT(p,/,1)});
                Set(trunkring=${CUT(p,/,2)});
                Set(trunk=${CUT(trunkring,\,,1)});
                Set(ring=${CUT(trunkring,\,,2)});
                if( ${FIELDNUM(muhnewtechs, ,${tech})} > 0 ) {
                    Set(abcdialstr2=${tech}/${cleanext}@${trunk});
                } else if( ${FIELDNUM(muholdtechs, ,${tech})} > 0 ) {
                    Set(abcdialstr2=${tech}/${trunk}/${cleanext});
                } else {
                    // unknown tech hmm
                }
                //&pngnpbx-abc-howdy-yall();
                //goto pngnpbx-abc-dial,${sEXTEN},1;
                // hey a Dial!
                Dial(${abcdialstr2}&${abcdialstr1},${ring},r);
            }
        } else if( "x${abcmode}" == "xtincan" ) {
            &pngnpbx-abc-tincan(ABC-EASY-TINCAN);
            &pngnpbx-abc-gps-get(); // gets GPS from AstDB, if any
        } else if( "x${abcmode}" == "xsimple" ) {
            &pngnpbx-abc-simple(ABC-EASY-SIMPLE);
            &pngnpbx-abc-gps-get(); // gets GPS from AstDB, if any
        }

        if( "x${abcmode}" == "xsimple" || "x${abcmode}" == "xtincan" ) {
            for( i=0; ${i} < 10; i=${i} + 1 ) {
                Set(p=${AST_CONFIG(abc.conf,eoc,route,${i})});
                if( ${LEN(${p})} == 0 ) {
                    break;
                }
                Set(tech=${CUT(p,/,1)});
                Set(trunkringx=${CUT(p,/,2)});
                Set(trunk=${CUT(trunkringx,\,,1)});
                Set(ring=${CUT(trunkringx,\,,2)});
                Set(cpx=${CUT(trunkringx,\,,3)});
                Set(csx=${CUT(trunkringx,\,,4)});
                if( ${FIELDNUM(muhnewtechs, ,${tech})} > 0 ) {
                    Set(abcdialstr3=${tech}/${cleanext}@${trunk});
                } else if( ${FIELDNUM(muholdtechs, ,${tech})} > 0 ) {
                    Set(abcdialstr3=${tech}/${trunk}/${cleanext});
                } else {
                    // unknown tech hmm
                }
                j=${i}+1;
                &pngnpbx-abc-pathx(${j},${ring},${abcdialstr3},${cpx},${csx});
            }
            // remote paths must be added before dialing remote
            &pngnpbx-abc-remote(${cleancid},static);
            for( i=0; ${i} < 10; i=${i} + 1 ) {
                Set(f=${AST_CONFIG(abc.conf,eoc,friend,${i})});
                if( ${LEN(${f})} == 0 ) {
                    break;
                }
                if( ${FIELDQTY(f,/)} > 1 ) {
                    &pngnpbx-abc-friend(${i},${f});
                } else {
                    &pngnpbx-abc-friend(${i},PJSIP/${f});
                }
            }
            for( i=0; ${i} < 10; i=${i} + 1 ) {
                Set(t=${AST_CONFIG(abc.conf,eoc,troll,${i})});
                if( ${LEN(${t})} == 0 ) {
                    break;
                }
                if( ${FIELDQTY(t,/)} > 1 ) {
                    &pngnpbx-abc-troll(${i},${t},nosy);
                } else {
                    &pngnpbx-abc-troll(${i},PJSIP/${t},nosy);
                }
            }
            for( i=0; ${i} < 10; i=${i} + 1 ) {
                Set(n=${AST_CONFIG(abc.conf,eoc,paladin,${i})});
                if( ${LEN(${n})} == 0 ) {
                    break;
                }
                if( ${FIELDQTY(n,/)} > 1 ) {
                    &pngnpbx-abc-paladin(${i},${n},nonnosy);
                } else {
                    &pngnpbx-abc-paladin(${i},PJSIP/${n},nonnosy);
                }
            }
            &pngnpbx-abc-howdy-yall();
        }
    }
}

